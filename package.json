{
  "name": "seven-design-ui",
  "version": "0.0.9",
  "description": "企业级 React UI 组件库",
  "license": "MIT",
  "author": "seven.zhong",
  "scripts": {
    "build": "pnpm --filter ./packages/** build",
    "build:components": "pnpm --filter @seven-design-ui/components build",
    "build:core": "pnpm --filter @seven-design-ui/core build",
    "build:theme": "pnpm --filter @seven-design-ui/theme build",
    "changeset": "changeset",
    "clean": "pnpm --filter ./packages/** exec rm -rf dist",
    "dev": "pnpm --filter @seven-design-ui/play dev",
    "docs:build": "pnpm --filter docs build",
    "docs:deploy": "pnpm docs:build && node scripts/deploy-docs.js",
    "docs:dev": "pnpm --filter docs dev",
    "docs:preview": "pnpm --filter docs preview",
    "format": "prettier --write \"**/*.{ts,tsx,json,md}\"",
    "lint": "eslint . --ext .ts,.tsx",
    "prepare": "husky install",
    "release": "pnpm build && changeset version && node scripts/update-version.js && changeset publish",
    "update-docs-version": "node scripts/update-version.js",
    "test": "vitest",
    "version-packages": "changeset version"
  },
  "dependencies": {
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "jiti": "latest",
    "less": "latest",
    "lightningcss": "latest",
    "lucide-react": "0.562.0",
    "next": "16.1.3",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "rspress": "1.47.0",
    "sass": "latest",
    "sass-embedded": "latest",
    "stylus": "latest",
    "sugarss": "latest",
    "tailwind-merge": "3.4.0",
    "tailwindcss-animate": "1.0.7",
    "terser": "latest",
    "tsx": "latest",
    "yaml": "latest"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.1",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/node": "^20.11.5",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "gh-pages": "^6.3.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "postcss": "8.5.6",
    "prettier": "^3.2.4",
    "tailwindcss": "4.1.18",
    "typescript": "^5.3.3",
    "vite": "^5.0.11",
    "vitest": "^1.2.1"
  },
  "packageManager": "pnpm@8.15.0",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  }
}
